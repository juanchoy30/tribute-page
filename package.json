    "url": "https://github.com/juanchoy30/tribute-page/issues"
  },
  "homepage": "https://github.com/juanchoy30/tribute-page#readme",
  "devDependencies": {
    "lite-server": "^2.5.4",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "parallelshell": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.0",
    "popper.js": "^1.16.1"
  }
}
